
/*
  This behavior models passengers grabbing their luggage from the overhead bins.
*/

Types:
  Luggage_Grabber
.

Location:
  Name: Aisle
  Dimensions:
    Center: {13.5, 1.73}
    Lengths: {27, 0.5}
    Rotation: 0
.

Selector:
  Type: Luggage_Grabber
  Select: 75%
.

Action (Luggage_Grabber):
  Condition: enter @Aisle
  Response: set velocity to {0}
  Duration: random 10 to 30 seconds
.